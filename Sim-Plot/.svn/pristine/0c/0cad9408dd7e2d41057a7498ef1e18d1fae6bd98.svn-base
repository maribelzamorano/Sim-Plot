package de.tudarmstadt.dvs.p4.simdb;

import java.util.Date;

/**
 * The Class DumpEntry.
 */
public class DumpEntry {

	/** The time. */
	private final Date time;
	
	/** The node. */
	private final int node;
	
	/** The text. */
	private final String text;

	/**
	 * Instantiates a new dump entry.
	 *
	 * @param time the time
	 * @param node the node
	 * @param text the text
	 */
	public DumpEntry(final Date time, final int node, final String text) {
		this.time = time;
		this.node = node;
		this.text = text;
	}

	/**
	 * Gets the node.
	 *
	 * @return the node
	 */
	public int getNode() {
		return node;
	}

	/**
	 * Gets the text.
	 *
	 * @return the text
	 */
	public String getText() {
		return text;
	}

	/**
	 * Gets the time.
	 *
	 * @return the time
	 */
	public Date getTime() {
		return time;
	}

}
